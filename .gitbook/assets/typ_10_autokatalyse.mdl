RG1=
	IF THEN ELSE(E1<=0, 0 , GG )
	~	
	~		|

E= INTEG (
	-GG,
		0.1)
	~	
	~	Oxalsäure
	|

E1= INTEG (
	-RG1,
		0.01)
	~	
	~	Kalumpermanganat wird gleich rasch umgewandelt, wie die Oxalsäure. Dabei \
		bildet sich aber der Katalysator Mn2+
	|

EKat= INTEG (
	GG-RG,
		0)
	~	
	~	Substrat-Enzym-Komplex
	|

GG=
	k1*E*Kfrei-k2*EKat
	~	
	~	Gleichgewichtsreaktion
	|

K=
	100
	~	 [10,1000,10]
	~	Gleichgewichtskonstante
	|

k1=
	0.1
	~	 [0.1,2]
	~	Reaktions-Geschwindikgeits-Konstante der Vorwärtsreaktion
	|

k2=
	k1/K
	~	
	~	Reaktions-Geschwindigkeits-Konstante der Rückreaktion
	|

k3=
	1
	~	 [0,5]
	~	Reaktions-Geschwindigkeitskonstante der Produktebildung
	|

Kat= INTEG (
	RG1,
		0.001)
	~	
	~	Mn2+ als Katalysator
	|

Kfrei=
	Kat-EKat
	~	
	~		|

P= INTEG (
	RG,
		0)
	~	
	~	Produkt
	|

RG=
	k3*EKat
	~	
	~		|

